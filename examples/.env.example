# Example environment configuration for running examples
# Copy this to .env in the examples directory

# OAuth2 Credentials (relative to examples directory)
GBP_CREDENTIALS_FILE=../credentials.json
GBP_TOKEN_FILE=../token.json

# OAuth2 settings
GBP_REDIRECT_URI=http://localhost:8080/callback

# Your Google Cloud Project
GOOGLE_CLOUD_PROJECT_ID=your-project-id

# Example resource names (replace with your actual values after first run)
EXAMPLE_ACCOUNT_NAME=accounts/1234567890
EXAMPLE_LOCATION_NAME=accounts/1234567890/locations/0987654321

# Development settings
DEBUG_MODE=true
LOG_LEVEL=INFO